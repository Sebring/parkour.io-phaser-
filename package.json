{
  "name": "lance-brawler",
  "version": "4.0.0",
  "engines": {
    "node": ">=11.0.0"
  },
  "description": "An HTML5 multiplayer parkour game based on Lance",
  "main": "src/main.js",
  "dependencies": {
    "@babel/node": "^7.2.2",
    "babel-loader": "^8.0.5",
    "express": "^4.14.0",
    "lance-gg": "^4.0.8",
    "nodemon": "^1.19.1",
    "pixi.js": "^4.2.3",
    "query-string": "^4.2.3",
    "socket.io": "^2.1.1",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-watch": "^7.0.0"
  },
  "scripts": {
    "start": "node dist-server/main.js",
    "start-debug": "node --inspect-brk dist-server/main.js",
    "start-dev": "nodemon dist-server/main.js",
    "postinstall": "webpack && babel src --source-maps --out-dir dist-server",
    "build": "webpack && babel src --source-maps --out-dir dist-server",
    "build-watch": "webpack --watch && babel src --source-maps --out-dir dist-server",
    "stats": "webpack --env production --profile --json > stats.json",
    "dev": "webpack --watch",
    "babel": "babel-watch src/main.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "game",
    "parkour",
    "platformer",
    "2D",
    "lance",
    "lance-gg",
    "multiplayer",
    "websockets"
  ],
  "author": "Johan Sebring",
  "license": "Apache-2.0"
}
